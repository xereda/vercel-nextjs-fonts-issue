{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/mocks/*": [
        "src/mocks/*"
      ],
      "@/providers/*": [
        "src/providers/*"
      ],
      "@/modules/*": [
        "src/modules/*"
      ],
      "@/constants/*": [
        "src/constants/*"
      ],
      "@/transform/*": [
        "src/transform/*"
      ],
    },
  },
  "exclude": [
    "node_modules"
  ],
}