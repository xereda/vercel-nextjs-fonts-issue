{
  "name": "test-ant",
  "private": true,
  "scripts": {
    "prepare-envs": "shx rm -f .env.local",
    "dev": "shx cp .env .env.local && next dev",
    "build": "branch=$(git rev-parse --symbolic-full-name --abbrev-ref HEAD) && yarn prepare-envs; shx cat .env.$branch .env.generic-vars > .env.local && next build",
    "start": "next start",
    "dev:qa": "yarn prepare-envs && shx cat .env.qa .env.generic-vars > .env.local && next dev",
    "build:qa": "yarn prepare-envs && shx cat .env.qa .env.generic-vars > .env.local && next build",
    "dev:staging": "yarn prepare-envs && shx cat .env.staging .env.generic-vars > .env.local && next dev",
    "build:staging": "yarn prepare-envs && shx cat .env.staging .env.generic-vars > .env.local && next build",
    "lint": "next lint",
    "test": "jest --",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@hookstate/core": "^3.0.13",
    "@hookstate/devtools": "^3.0.1",
    "@hookstate/persistence": "^3.0.0",
    "antd": "^4.17.3",
    "axios": "^0.25.0",
    "cookie": "^0.4.2",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "gerador-validador-cpf": "^5.0.2",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^12.1.6",
    "node-rsa": "^1.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dates": "^21.8.0",
    "react-dom": "^18.1.0",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.3.1",
    "react-minimal-pie-chart": "^8.2.0",
    "react-tooltip": "^4.2.21",
    "sharp": "^0.30.1",
    "swr": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "msw": "^0.36.8",
    "sass": "^1.45.1",
    "shx": "^0.3.4",
    "timekeeper": "^2.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
